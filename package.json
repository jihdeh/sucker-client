{
  "name": "sucker-client",
  "version": "1.0.0",
  "description": "Sucker Client",
  "main": "server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --ignore public -d 2 server/index",
    "build": "echo Build mode: ${NODE_ENV:-develop} && rm -R -f dist && mkdir -p dist && npm run app",
    "app": "webpack",
    "watch:hash": "nodemon --watch dist",
    "watch": "npm run watch:hash & webpack --display-modules --progress --watch --display-error-details",
    "style": "node-sass -E \"UTF-8\" style/index.s[ac]ss > dist/style.css && postcss --use autoprefixer dist/style.css -r",
    "style:prod": "node-sass -E \"UTF-8\" --output-style compressed style/index.s[ac]ss > dist/style.css && postcss --use autoprefixer dist/style.css -r"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jihdeh/sucker-client.git"
  },
  "keywords": [
    "sucker",
    "learning",
    "killit"
  ],
  "author": "Jihdeh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jihdeh/sucker-client/issues"
  },
  "homepage": "https://github.com/jihdeh/sucker-client#readme",
  "dependencies": {
    "iso": "^5.2.0",
    "koa": "^1.2.4",
    "koa-cache-control": "^1.0.0",
    "koa-forward-request": "^0.7.1",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "koa-static-cache": "^3.1.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8"
  },
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-stage-0": "^6.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.9.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.5.1",
    "glob": "^5.0.14",
    "istanbul": "^1.0.0-alpha.2",
    "jscs": "^2.5.1",
    "jscs-loader": "^0.2.0",
    "jsdom": "^8.0.1",
    "json-loader": "^0.5.2",
    "mocha": "^2.5.3",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.4.1",
    "postcss-cli": "^2.4.0",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "sinon": "^1.17.2",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.3.2",
    "supertest": "^1.0.1",
    "supertest-as-promised": "^2.0.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
